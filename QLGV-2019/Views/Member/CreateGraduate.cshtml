
@{
    ViewBag.Title = "CreateGraduate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container text-center">
    <h3>Create Graduate</h3>
    <br />
    <form action="~/Member/CreateGraduate" method="post" class="form-horizontal">
        <div class="form-group">
            <label class="control-label col-sm-3">ID Student:</label>
            <div class="col-sm-7">
                <input type="text" name="Id_Student" value="" class="form-control" />
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-sm-3">ID:</label>
            <div class="col-sm-7">
                <input type="text" name="Id" class="form-control" value="" />
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-sm-3">Level:</label>
            <div class="input-group">
                <select name="Level" class="input-sm col-sm-offset-2">
                    <option value=""></option>
                    <option value="Khá">Khá</option>
                    <option value="Giỏi">Giỏi</option>
                    <option value="Trung Bình">Trung Bình</option>
                </select>
            </div> 
        </div>

        <div class="form-group">
            <label class="control-label col-sm-3">Date Sign:</label>
            <div class="col-sm-7">
                <input type="date" name="Date_sign" value="" class="form-control" />
            </div>
        </div>
        <input type="submit" name="name" value="Create" class="btn btn-default" />
    </form>
</div>

