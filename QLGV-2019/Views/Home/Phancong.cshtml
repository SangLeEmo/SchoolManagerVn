    <div class="container">
        <h2>Thêm sinh viên mới</h2>
        <form class="form-horizontal">
            <div class="form-group">
                <label class="control-label col-sm-2">Mã môn học <span class="glyphicon glyphicon-list-alt"></span></label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="idMH" placeholder="Nhập mã môn học" name="idMH">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Môn học <span class="glyphicon glyphicon-book"></span></label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="MH" placeholder="Nhập tên môn học" name="MH">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Lớp <span class="glyphicon glyphicon-th-list"></span></label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="classes" placeholder="Nhập lớp cố vấn" name="classes">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Học kỳ <span class="glyphicon glyphicon-time"></span></label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="semester" placeholder="Nhập học kỳ" name="semester">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Năm học <span class="glyphicon glyphicon-edit"></span></label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="year" placeholder="Nhập năm học" name="year">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Ngày bắt đầu <span class="glyphicon glyphicon-calendar"></span></label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="DBegin" placeholder="Nhập ngày bắt đầu" name="DBegin">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Ngày kết thúc <span class="glyphicon glyphicon-calendar"></span></label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="DFinish" placeholder="Nhập ngày kết thúc" name="DFinish">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Mã giảng viên <span class="glyphicon glyphicon-user"></span></label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="idGV" placeholder="Nhập mã giảng viên" name="idGV">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2" for="email">Giảng viên <span class="glyphicon glyphicon-pencil"></span></label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="GV" placeholder="Nhập tên giảng viên" name="GV">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Học vị <span class="glyphicon glyphicon-briefcase"></span></label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="degree" placeholder="Nhập học vị" name="degree">
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <div class="checkbox">
                        <label><input type="checkbox" name="confirm">Xác nhận thông tin trên chính xác</label>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-info">
                        <span class="glyphicon glyphicon-user"></span> Thêm
                    </button>
                </div>
            </div>
        </form>
    </div>
 
